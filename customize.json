		{ "selector": "#access ul > li > a:hover, #access li:hover > a, #access ul > li > a:focus", "type": "background-color", "label": "Top item hover background color", "template": "%s { background-color: %s; background-image: none; }" },
		/* Sub menu items */
		{ "selector": "#access ul ul a", "type": "text", "label": "Submenu item" },
		{ "selector": "#access ul > li li a", "type": "color", "label": "Submenu item color" },
		{ "selector": "#access ul > li li a:hover, #access ul > li li a:active, #access ul > li li a:focus, #access li:hover > a", "type": "color", "label": "Submenu item hover color" },
